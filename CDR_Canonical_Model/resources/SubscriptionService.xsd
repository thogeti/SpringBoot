        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Flag" type="sdm:Flag"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="UnsubscribeToPatient" type="ss:UnsubscribeToPatient"/>
