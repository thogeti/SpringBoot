<?xml version="1.0" encoding="utf-8"?>
<xs:schema
    targetNamespace="http://shoppersdrugmart.ca/RxHB/DRx/SubscriptionService"
    xmlns:sdm="http://www.shoppersdrugmart.ca/message/SDMDataTypes"
