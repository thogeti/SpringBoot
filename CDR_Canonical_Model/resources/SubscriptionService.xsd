            <xs:element maxOccurs="1" minOccurs="1" name="StoreNumber" type="sdm:StringValue"/>
            <xs:element maxOccurs="1" minOccurs="1" name="PatientId" type="sdm:IdentityAlphanumeric"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="UnsubscribeToPatientResponse" type="ss:UnsubscribeToPatientResponse"/>
    <xs:complexType name="UnsubscribeToPatientResponse">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Flag" type="sdm:Flag"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="UnsubscribeToRx" type="ss:UnsubscribeToRx"/>
