    <xs:element name="SubscribeToPatient" type="ss:SubscribeToPatient"/>
    <xs:complexType name="SubscribeToPatient">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="StoreNumber" type="sdm:StringValue"/>
            <xs:element maxOccurs="1" minOccurs="1" name="PatientId" type="sdm:IdentityAlphanumeric"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="SubscribeToPatientResponse" type="ss:SubscribeToPatientResponse"/>
    <xs:complexType name="SubscribeToPatientResponse">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Flag" type="sdm:Flag"/>
