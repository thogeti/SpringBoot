	<xs:simpleType name="RentalCount">
		<xs:annotation>
			<xs:documentation>The number of RentalUnits that were rented by the customer during the reporting period.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Quantity"/>
	</xs:simpleType>
	<xs:simpleType name="RentalDepositAmount">
		<xs:annotation>
			<xs:documentation>A deposit related to the value of the RENTAL OBJECT which is paid by the CUSTOMER and is refundable if the ITEM is returned in the same condition.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
	<xs:simpleType name="RentalDepositPercent">
		<xs:annotation>
			<xs:documentation>The percentage of the value of the RENTAL OBJECT which is charged as a refundable deposit to the  CUSTOMER.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Percent"/>
	</xs:simpleType>
	<xs:simpleType name="RentalLatePenaltyAmount">
		<xs:annotation>
			<xs:documentation>The penalty amount which is imposed by the RETAILER if the CUSTOMER fails to meet the rental terms.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
	<xs:simpleType name="RentalLatePenaltyPercent">
		<xs:annotation>
			<xs:documentation>The percentage penalty which is imposed if the terms of the rental are abused by the CUSTOMER, eg if the rental item is delivered late the penalty is 10% of the rental fee for each date late.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Percent"/>
	</xs:simpleType>
	<xs:simpleType name="RentalPeriodCount">
		<xs:annotation>
			<xs:documentation>The number of rental periods the item may be rented by the customer; as well as the number of rental periods per penalty charge.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Count"/>
	</xs:simpleType>
	<xs:simpleType name="RentalPeriodTypeCode">
		<xs:annotation>
			<xs:documentation>A code to denote the rental and overdue periods; Hour, Day, Week, etc...</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="RepeatVisitorFlag">
		<xs:annotation>
			<xs:documentation>A boolean indicator that tells if this request is from a repeat visitor nor not.

This assumes that the retailer's web site is using some persistence mechanism like cookies to identify visitors so repeat visits can be distinguished from first time visits.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Flag"/>
	</xs:simpleType>
	<xs:simpleType name="ReportingPeriodID">
		<xs:annotation>
			<xs:documentation>The unique system assigned identifier for a particular ReportingPeriod.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Identity"/>
	</xs:simpleType>
	<xs:simpleType name="RequestID">
		<xs:annotation>
			<xs:documentation>An HTTP request initated by the client.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="RequestTimeStamp">
		<xs:annotation>
			<xs:documentation>Date and time the request was served.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="RequestedAmount">
		<xs:annotation>
			<xs:documentation>The monetary value of the purchases for which an authorization is being requested.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:MoneyShort"/>
	</xs:simpleType>
	<xs:simpleType name="RequestedByEmployeeID">
		<xs:annotation>
			<xs:documentation>The EMPLOYEE requesting the debit or credit</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Identity"/>
	</xs:simpleType>
	<xs:simpleType name="RequestedChangeAmount">
		<xs:annotation>
			<xs:documentation>The monetary value cash-back for which an authorization is being requested</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:MoneyShort"/>
	</xs:simpleType>
	<xs:simpleType name="RequiredPaymentAmount">
		<xs:annotation>
			<xs:documentation>How much is required to be paid for COD shipment?</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:MoneyShort"/>
	</xs:simpleType>
	<xs:simpleType name="ResourceID">
		<xs:annotation>
			<xs:documentation>A unqiue system assigned identifier for the action to which access may be granted to some Operators and Workstations.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Identity"/>
	</xs:simpleType>
	<xs:simpleType name="ResourceOverrideSequenceNumber">
		<xs:annotation>
			<xs:documentation>Line number (incremental counter)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Number"/>
	</xs:simpleType>
	<xs:simpleType name="Response">
		<xs:annotation>
			<xs:documentation>The response the Customer gave to the particular SurveyQuestion.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="ResponseDate">
		<xs:annotation>
			<xs:documentation>The date the Customer provided the response to the CustomerSurvey</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Date"/>
	</xs:simpleType>
	<xs:simpleType name="ResponseIPAddress">
		<xs:annotation>
			<xs:documentation>Reponse to IP address.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="ResponseTimeStamp">
		<xs:annotation>
			<xs:documentation>Date and time a response is generated in response to a feed request.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="ResumedTotalAmount">
		<xs:annotation>
			<xs:documentation>The total monetary amount of TRANSACTIONs resumed by OPERATOR for each REPORTING PERIOD.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
