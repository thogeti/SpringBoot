<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sdm="http://www.shoppersdrugmart.ca/message/SDMDataTypes" targetNamespace="http://www.shoppersdrugmart.ca/glossary">
	<xs:import namespace="http://www.shoppersdrugmart.ca/message/SDMDataTypes" schemaLocation="SDMDataTypes.xsd"/>
	<xs:simpleType name="PromotionSecretCode">
		<xs:annotation>
			<xs:documentation>Secret code assigned to an optimum card for a specific promotion</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="SendConfirmationEmailFlag">
		<xs:annotation>
			<xs:documentation>Must a confirmation email be send to the customer ?</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Flag"/>
	</xs:simpleType>
	<xs:simpleType name="DoubleOpt_InRequiredFlag">
		<xs:annotation>
			<xs:documentation>Must the customer confirm that they want to opt-in to the promotion?. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Flag"/>
	</xs:simpleType>
	<xs:simpleType name="SecretCodeRequiredFlag">
		<xs:annotation>
			<xs:documentation>This promotion requires a secret code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Flag"/>
	</xs:simpleType>
	<xs:simpleType name="PromotionDescription">
		<xs:annotation>
			<xs:documentation>Description of the Promotion</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:DescriptionUnicode"/>
	</xs:simpleType>
	<xs:simpleType name="AboveUpperLimitActionRequiredTypeCode">
		<xs:annotation>
			<xs:documentation>A code that specifies the type of supervisory action required when the WORKSTATION records an item movement above an upper limit.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="AcceptanceForPaymentOnAccountFlag">
		<xs:annotation>
			<xs:documentation>A flag to signify that a certain tender type can be accepted to credit a customer's account.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Flag"/>
	</xs:simpleType>
	<xs:simpleType name="AccessPasswordSequenceNumber">
		<xs:annotation>
			<xs:documentation>A unique system assigned sequence number for each successive AccessPassword chosen by the Operator.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Number"/>
	</xs:simpleType>
	<xs:simpleType name="AccountIdentificationRequiredFlag">
		<xs:annotation>
			<xs:documentation>A flag to signify that the status of the account has to be checked before this method of tender can be accepted.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Flag"/>
	</xs:simpleType>
	<xs:simpleType name="AccountNumber">
		<xs:annotation>
			<xs:documentation>The CUSTOMER's account number against which previous tender authorization is held.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Identity"/>
	</xs:simpleType>
	<xs:simpleType name="AccountingDispositionCode">
		<xs:annotation>
			<xs:documentation>The code to denote as to how the amendment resulting from the  INVENTORY DOCUMENT  MERCHANDISE LINE ITEM MODIFIER will be accounted for within the STOCK LEDGER.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="AccruedInterestAmount">
		<xs:annotation>
			<xs:documentation>The interest amount accrued against the unpaid blance of this customer account.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
	<xs:simpleType name="ActionApprovalByTypeCode">
		<xs:annotation>
			<xs:documentation>A code to signify the type of approval granted.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="ActionCode">
		<xs:annotation>
			<xs:documentation>A code denoting how the item is being treated in the line item.

Sample values include:
SL = Sale
RT = Return</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="ActionDatetime">
		<xs:annotation>
			<xs:documentation>The date and time that the associate performed the action for which a commission is payable.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="ActivatedDateTimestamp">
		<xs:annotation>
			<xs:documentation>The time and date when the gift certificate was activated by the retail enterprise</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="ActivityID">
		<xs:annotation>
			<xs:documentation>Token ID for an Activity instance.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:IdentityUUID"/>
	</xs:simpleType>
	<xs:simpleType name="ActivityInterestCode">
		<xs:annotation>
			<xs:documentation>A lookup value that designates a valid activity or interest code used to segment customers.

The code list must be defined by the retailer.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="ActivityInterestDescription">
		<xs:annotation>
			<xs:documentation>A short narrative description of an activity/interest type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="ActivityName">
		<xs:annotation>
			<xs:documentation>Business name of an Activity instance.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="ActivityNarrative">
		<xs:annotation>
			<xs:documentation>Short narrative description of an Activity instance.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:DescriptionNarrative"/>
	</xs:simpleType>
	<xs:simpleType name="ActualAvailabilityDate">
		<xs:annotation>
			<xs:documentation>The date all the merchandise being ordered was made available for delivery to the Customer or pickup by the Customer.

Where the merchandise becomes available over a period of time - this field holds the date the last ordered product was available for delivery or pickup.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Date"/>
	</xs:simpleType>
	<xs:simpleType name="ActualDeliveryDate">
		<xs:annotation>
			<xs:documentation>The actual delivery date of the shipment, to which this particular inventory control document relates.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Date"/>
	</xs:simpleType>
	<xs:simpleType name="ActualEndDateTimestamp">
		<xs:annotation>
			<xs:documentation>The actual time and date when the event terminates.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="ActualReceivedDateTime">
		<xs:annotation>
			<xs:documentation>The actual date &amp; time the shipment was received at the shipment address.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="ActualSalesAmount">
		<xs:annotation>
			<xs:documentation>The total monetary value of sales, including returns which were made by the   MerchandisingRevenueCenter during the ReportingPeriod.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
	<xs:simpleType name="ActualServiceLevelCode">
		<xs:annotation>
			<xs:documentation>A code denoting the level of delivery service that is being used to deliver this shipment

Examples include: Express, Ground, Overnight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="ActualShipDateTime">
		<xs:annotation>
			<xs:documentation>The preferred delivery date &amp; time</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="ActualSizeCode">
		<xs:annotation>
			<xs:documentation>The last four characters of the NRF size code. This code uniquely identifies a particular size, eg dress size 12 or combination of sizes, eg pants, waist size 36, inseam 31.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="ActualSizeProportionDescription">
		<xs:annotation>
			<xs:documentation>The proportions typically used to measure this particular ITEM type, eg 36 (waist) x 31(inseam).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="ActualStartDateTimestamp">
		<xs:annotation>
			<xs:documentation>The actual start date and time for an event.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="ActualUnitPrice">
		<xs:annotation>
			<xs:documentation>The actual per-unit price paid by the customer for this particular sale. It is obtained by applying  applicable price derivation rules to the regular unit price.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
	<xs:simpleType name="ActualUnitPriceQuantity">
		<xs:annotation>
			<xs:documentation>The number of units applicable to the actual per-unit price paid by the customer for this particular sale. 

e.g.  3 for 87c</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:DecimalCount"/>
	</xs:simpleType>
	<xs:simpleType name="AddDateTime">
		<xs:annotation>
			<xs:documentation>Time stamp (to microseconds) when item is added to the cart</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="AddressID">
		<xs:annotation>
			<xs:documentation>A unique system allocated identifier for the Address.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Identity"/>
	</xs:simpleType>
	<xs:simpleType name="AddressLine1">
		<xs:annotation>
			<xs:documentation>The first line of the address, normally the street number and name.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:AddressLine"/>
	</xs:simpleType>
	<xs:simpleType name="AddressLine2">
		<xs:annotation>
			<xs:documentation>The second line of an address, normally the Flat or Building Suite number.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:AddressLine"/>
	</xs:simpleType>
	<xs:simpleType name="AddressLine3">
		<xs:annotation>
			<xs:documentation>The third line of the address.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:AddressLine"/>
	</xs:simpleType>
	<xs:simpleType name="AddressLine4">
		<xs:annotation>
			<xs:documentation>The fourth line of the address.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:AddressLine"/>
	</xs:simpleType>
	<xs:simpleType name="AddressVerificationCode">
		<xs:annotation>
			<xs:documentation>A code denoting how the customer's address was verified.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="AdjudicationCode">
		<xs:annotation>
			<xs:documentation>Adjudication to the request for credit or debit.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="AdjustmentAmount">
		<xs:annotation>
			<xs:documentation>The sum of tender adjustment amount transactions for a till settlement period.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
	<xs:simpleType name="AdjustmentCode">
		<xs:annotation>
			<xs:documentation>A mnemonic code denoting what kind of adjustment is being made to the Item being sold.

e.g. Add, Remove, Alter,List Components</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="AdjustmentDateTime">
		<xs:annotation>
			<xs:documentation>The date and time an adjustment was made.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="AdjustmentDescription">
		<xs:annotation>
			<xs:documentation>A narrative describing the required modification.

e.g. Hold the Pickle,  Cuffs on Trousers etc...</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:DescriptionNarrative"/>
	</xs:simpleType>
	<xs:simpleType name="AdjustmentMediaUnitQuantity">
		<xs:annotation>
			<xs:documentation>The sum of tender adjustment media unit quantity transactions for a till settlement period.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Quantity"/>
	</xs:simpleType>
