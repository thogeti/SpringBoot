	<xs:simpleType name="Amount_5">
		<xs:annotation>
			<xs:documentation>The flat amount of the price adjustment that was removed from the unit selling price to arrive at the modified selling price.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
	<xs:simpleType name="AnchorText">
		<xs:annotation>
			<xs:documentation>The anchor text value(s) associated with the web page and site being exited to from ExitPage</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="AnnualFeeAmount">
		<xs:annotation>
			<xs:documentation>Monetary value of the annual amount a customer pays to participate in a LoyaltyProgram.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Money"/>
	</xs:simpleType>
	<xs:simpleType name="AnnualIncomeRangeCode">
		<xs:annotation>
			<xs:documentation>A customer's annual salary or income.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="AnonymousAttributeValueID">
		<xs:annotation>
			<xs:documentation>Token identifier for an instance of an AnonymousAttributeValue which uses an Entity-Value-Attribute pattern to allow retailers to extend their description of an Anonymous Party subtype.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:IdentityUUID"/>
	</xs:simpleType>
	<xs:simpleType name="AnonymousFlag">
		<xs:annotation>
			<xs:documentation>A boolean indicator that tells if this is an anonymous customer or not.  If TRUE this is an anonymous customer that has no KeyCustomer (and related dependent information) and no PartyContactMethod information associated with it.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Flag"/>
	</xs:simpleType>
	<xs:simpleType name="AnonymousLabelName">
		<xs:annotation>
			<xs:documentation>A short name used to reference an Anonymous party type in presentation and reporting.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="Answer">
		<xs:annotation>
			<xs:documentation>The answer given by the customer to the given question.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="ApparelAgeGroupCode">
		<xs:annotation>
			<xs:documentation>Assigns the apparel item to a target consumer age group:

Examples include:

NEWBORN
INFANT
TODDLER
PRESCHOOL
6-10
TWEENS
TEEN
YOUNG ADULT
ADULT</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="ApparelCareDescription">
		<xs:annotation>
			<xs:documentation>A short narrative that designates specific cleaning and maintenance restrictions and requirements for an apparel item. 

Example

Mchn Wsh Cold Perm Press Tumbl Dry Low No Ironing No DryClean</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Description"/>
	</xs:simpleType>
	<xs:simpleType name="ApparelSeasonCode">
		<xs:annotation>
			<xs:documentation>Apparel targeted wearing season:

Examples include:
SPRING
SUMMER
FALL
WINTER
ALLSEASON</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Code"/>
	</xs:simpleType>
	<xs:simpleType name="AppliedTimestamp">
		<xs:annotation>
			<xs:documentation>The date and time the maintenance event's associated adds, changes and deletes are applied to the store database.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sdm:Timestamp"/>
	</xs:simpleType>
	<xs:simpleType name="ApplyAfterAllMarkdownsFlag">
