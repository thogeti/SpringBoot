	<xsd:element name="DrugNameAlternative" type="ehlth:DrugNameAlternative"/>
	<xsd:complexType name="DrugNameAlternative">
		<xsd:annotation>
			<xsd:documentation>A group of freeform textual names that identify the drug in a specific standardised way. Used for downstream consumers of the data.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="equivalentto" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>a freeform text field identifying a brand name or a generic name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tradenamealternative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This freetext field is used to capture the name of the drug in TallMan format</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tradereference" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>a freeform text field which represents the brand name of the molecule</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="EngagementType">
