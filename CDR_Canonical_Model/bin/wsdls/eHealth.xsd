			<xsd:element name="serviceLocation" type="ehlth:Location" minOccurs="0"/>
			<xsd:element name="fillStatus" type="ehlth:TxFillStatus" minOccurs="0"/>
			<xsd:element name="dispenser" type="ehlth:Dispenser" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="packageForm" type="ehlth:PackageForm" minOccurs="0"/>
			<xsd:element name="pack" type="ehlth:Pack" minOccurs="0"/>
			<xsd:element name="administrationRouteCode" type="ehlth:RouteOfAdmin" minOccurs="0"/>
			<xsd:element name="dosageForm" type="ehlth:DosageForm" minOccurs="0"/>
			<xsd:element name="note" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded"/>
