			</xsd:element>
			<xsd:element name="administrationPeriodEnd" type="xsd:date" minOccurs="0"/>
			<xsd:element name="daysSupply" type="xsd:int" minOccurs="0"/>
			<xsd:element name="administrationPeriodStart" type="xsd:date" minOccurs="0"/>
			<xsd:element name="createTimestamp" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is the date &amp; time that the dispense was created.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id in consumer for this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>id the producer assigned to this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="transactionCancellationReasonText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="lastUpdate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="pickupTime" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="quantityDispensed" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="totalAmountPaid" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="transactionNumber" type="xsd:int" minOccurs="0"/>
			<xsd:element name="dispenseTime" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is the service date</xsd:documentation>
				</xsd:annotation>
