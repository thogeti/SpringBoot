	<xsd:simpleType name="Allergen">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:element name="Allergy" type="ehlth:Allergy"/>
	<xsd:complexType name="Allergy">
		<xsd:sequence>
			<xsd:element name="allergyActiveFlag" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag to indicate if the allergy is currently active</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="intoleranceFlag" type="xsd:boolean" default="false" minOccurs="0"/>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id of entity in consumer (healthwatch)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="createTimestamp" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date / time when this allergy entry was created</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="descriptionEnglish" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>English text describing the allergy</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id the producer uses for this occurrence</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="descriptionFrench" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>French description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="allergyType" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is the allergy type that was reported</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="allergen" type="xsd:string" minOccurs="0"/>
			<xsd:element name="allergyStartDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="reportedDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="updateTimestamp" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="updateReason" type="xsd:string" minOccurs="0"/>
			<xsd:element name="allergyTest" type="ehlth:AllergyTest" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>If the patient has had a test to confirm the allergy, details will be here</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reaction" type="ehlth:Reaction" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>This is the reaction that has occurred due to the patient being exposed to something</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="linkedAllergy" type="ehlth:Allergy" minOccurs="0"/>
			<xsd:element name="recorder" type="ehlth:Recorder" minOccurs="0"/>
			<xsd:element name="allergySeverityCode" type="ehlth:AllergySeverityLevel" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indication of the severity of this allergy</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="allergyConfirmationStatusCode" type="ehlth:AllergyStatus" minOccurs="0"/>
			<xsd:element name="drug" type="ehlth:Drug" minOccurs="0"/>
			<xsd:element name="note" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="reporter" type="ehlth:Reporter" minOccurs="0"/>
			<xsd:element name="serviceLocation" type="ehlth:Location" minOccurs="0"/>
			<xsd:element name="nonDrugAllergen" type="ehlth:NonDrugAllergen" minOccurs="0"/>
			<xsd:element name="updateRecorder" type="ehlth:Recorder" minOccurs="0"/>
			<xsd:element name="supervisor" type="ehlth:Supervisor" minOccurs="0"/>
			<xsd:element name="statusChangeType" type="ehlth:StatusChangeType" minOccurs="0"/>
			<xsd:element name="statusChangeReason" type="ehlth:StatusChangeReason" minOccurs="0"/>
			<xsd:element name="dispenser" type="ehlth:Dispenser" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="AllergySeverityLevel">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="High"/>
			<xsd:enumeration value="Moderate"/>
			<xsd:enumeration value="Low"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AllergyStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Suspected"/>
			<xsd:enumeration value="Confirmed"/>
			<xsd:enumeration value="Resolved-Unconfirmed"/>
			<xsd:enumeration value="Resolved-Confirmed"/>
			<xsd:enumeration value="Refuted-Unconfirmed"/>
			<xsd:enumeration value="Refuted-Confirmed"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="AllergyTest" type="ehlth:AllergyTest"/>
	<xsd:complexType name="AllergyTest">
		<xsd:sequence>
			<xsd:element name="testType" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is the type of test that was performed</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="testResult" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is the textual result of the test</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id of entity in consumer (healthwatch)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id the producer uses for this occurrence</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="testDatetime" type="xsd:dateTime" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="AllergyTestResult">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Mild Reaction"/>
			<xsd:enumeration value="Minimal Reaction"/>
			<xsd:enumeration value="Moderate Reaction"/>
			<xsd:enumeration value="No Reaction"/>
			<xsd:enumeration value="Severe Reaction"/>
		</xsd:restriction>
