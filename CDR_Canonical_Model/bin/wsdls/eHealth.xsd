	<xsd:element name="Pack" type="ehlth:Pack"/>
	<xsd:complexType name="Pack">
		<xsd:annotation>
			<xsd:documentation>A description of the drug packing specifications and constraints associated with the prescription.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="alternativepacksize" type="xsd:int" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Other downstream systems may represent pack sizes in a different unit of measure which is compatible with other downstream consumers. This is the unit size of that pack.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="alternativepacksizeunitofmeasure" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Other downstream systems may represent pack sizes in a different unit of measure which is comparable with other downstream consumers. This is that Unit of measure.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="strength" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Global trade item number with 14 digits used in the bar code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GTIN" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Global trade item number with 14 digits used in the bar code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="isActiveFlag" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An indicator to show if the drug is discontinued by the manufacturer and is no longer available for dispense</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id in consumer for this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="isCurrentFlag" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An indicator to show if the drug is in latest version. Only the latest version can be ordered but older versions may still be dispensed</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>id the producer assigned to this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="manufacturerDiscontinuedDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date at which it was no longer possible to order the pack from the manufacturer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="masterid" type="xsd:string" minOccurs="0"/>
			<xsd:element name="packSize" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount of units in a pack during dispense</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="strengthUOMCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="drug" type="ehlth:Drug" minOccurs="0"/>
			<xsd:element name="packSizeUOMCode" type="ehlth:PackSizeUoMTypeDisplay" minOccurs="0"/>
