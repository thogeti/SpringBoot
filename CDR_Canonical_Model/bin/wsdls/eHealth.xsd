			<xsd:element name="daysInterval" type="xsd:int" minOccurs="0"/>
			<xsd:element name="dosageInstructionExpiryDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="drugLotNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="drugExpirationDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="sigCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="dispenseType" type="xsd:string" minOccurs="0"/>
			<xsd:element name="sigDescriptionPatientLanguage" type="xsd:string" minOccurs="0"/>
			<xsd:element name="sequenceNumber" type="xsd:int" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is the sequence number of the Dispenses within the Rx</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="previousDispenseDaysSupply" type="xsd:int" minOccurs="0"/>
			<xsd:element name="quantityRemaining" type="xsd:int" minOccurs="0"/>
			<xsd:element name="dispenseFulfillmentRefusalReasonCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="discontinuedReasonCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="administrationForm" type="xsd:string" minOccurs="0"/>
			<xsd:element name="statusChangeReasonCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="statusChangeType" type="xsd:string" minOccurs="0"/>
