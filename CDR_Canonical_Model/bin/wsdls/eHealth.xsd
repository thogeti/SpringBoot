	<xsd:element name="Compound" type="ehlth:Compound"/>
	<xsd:complexType name="Compound">
		<xsd:annotation>
			<xsd:documentation>A combination of drugs mixed together according to a particular set of specifications and instructions.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UnitOfMeasure" type="xsd:string" minOccurs="0"/>
			<xsd:element name="UofMQuantityConversionFactor" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="nameFrench" type="xsd:string" minOccurs="0"/>
			<xsd:element name="nameEnglish" type="xsd:string" minOccurs="0"/>
			<xsd:element name="preperationdirections" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>should be an XML document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id in consumer for this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>id the producer assigned to this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="compoundIngredients" type="ehlth:CompoundIngredient" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="dosageForm" type="ehlth:DosageForm" minOccurs="0"/>
			<xsd:element name="schedule" type="ehlth:Schedule" minOccurs="0"/>
			<xsd:element name="administrationRouteCode" type="ehlth:RouteOfAdmin" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is how the compound should be administered to the patient.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="totalPackageQuantity" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total quantity for compound</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="totalPackageUnit" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Unit of Measure for total compound package</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="CompoundIngredient" type="ehlth:CompoundIngredient"/>
	<xsd:complexType name="CompoundIngredient">
		<xsd:annotation>
			<xsd:documentation>The structured list of ingredients for a compound. </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="quantityUsed" type="xsd:decimal">
				<xsd:annotation>
