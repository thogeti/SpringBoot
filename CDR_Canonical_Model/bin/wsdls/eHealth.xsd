				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FPDN">
				<xsd:annotation>
					<xsd:documentation>Fill current prescription and do not ask again</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="REPH">
				<xsd:annotation>
					<xsd:documentation>Refer to pharmacist</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CPNA">
				<xsd:annotation>
					<xsd:documentation>Contact prescriber with no alternative selected</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CPAS">
				<xsd:annotation>
					<xsd:documentation>Contact prescriber with alternative selected</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PHNA">
				<xsd:annotation>
					<xsd:documentation>Pharmacist Initiated Therapeutic Alternative with no alternative selected</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PHAS">
				<xsd:annotation>
					<xsd:documentation>Pharmacist Initiated Therapeutic Alternative with alternative selected</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="POHB">
				<xsd:annotation>
					<xsd:documentation>Patient opts-out of all HW4B alerts</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PBHB">
				<xsd:annotation>
					<xsd:documentation>Patient opts-back on to HW4B alerts</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SPPA">
				<xsd:annotation>
					<xsd:documentation>Patient accepts</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SPPD">
				<xsd:annotation>
					<xsd:documentation>Patient declines</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SPAA">
				<xsd:annotation>
					<xsd:documentation>Ask again</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
