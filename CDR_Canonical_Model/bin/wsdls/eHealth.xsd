	<xsd:element name="Reaction" type="ehlth:Reaction"/>
	<xsd:complexType name="Reaction">
		<xsd:annotation>
			<xsd:documentation>This is the reaction that has occurred due to the patient being exposed to something.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="producerid" type="xsd:string" minOccurs="0"/>
			<xsd:element name="exposureMethod" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies how the patient was exposed to the allergen</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reactionStartTimestamp" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="reactionDescriptionEnglish" type="xsd:string" minOccurs="0"/>
			<xsd:element name="exposedMaterialName" type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="reactionCode" type="ehlth:SubjectReaction" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>code describing the patient's reaction to the allergy</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reactionSeverityCode" type="ehlth:ReactionSeverityLevel" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is the severity of the reaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="exposedMaterial" type="ehlth:NonDrugAllergen" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>This is the material that the patient was exposed to which caused the allergy</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="note" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>This is the note that was recorded against the reaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="drug" type="ehlth:Drug" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="causativeRx" type="ehlth:Prescription" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
