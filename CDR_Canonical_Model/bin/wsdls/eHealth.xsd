	<xsd:element name="Prescription" type="ehlth:Prescription"/>
	<xsd:complexType name="Prescription">
		<xsd:annotation>
			<xsd:documentation>If yes, there is a medical document related to this prescription in the possession of a dispensing authority.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="updateTimestamp" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="isAuthoritative" type="xsd:boolean" default="false" minOccurs="0"/>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id in consumer for this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>id the producer assigned to this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantityDispensed" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Added attribute of ODS interest (EC) currently on Rx.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="remainingQuantity" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prescription remaining quantity.

Used by Campaign.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="prescriptionNumber" type="xsd:int" minOccurs="0"/>
			<xsd:element name="refillsRemainingCount" type="xsd:int" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This is the number of refills remaining for this prescription (taking into account the previous dispenses).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="totalQuantityAuthorized" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total quantity Tx quantity.

Used by Campaign.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="drugTotalDaysOfSupplyCount" type="xsd:int" minOccurs="0"/>
			<xsd:element name="drugTrialDaysOfSupplyCount" type="xsd:int" minOccurs="0"/>
			<xsd:element name="drugTrialFlag" type="xsd:boolean" default="false" minOccurs="0"/>
			<xsd:element name="drugTrialQuantity" type="xsd:int" minOccurs="0"/>
			<xsd:element name="inferredPrescriptionFlag" type="xsd:boolean" default="false" minOccurs="0"/>
			<xsd:element name="noSubstitutionReason" type="xsd:string" minOccurs="0"/>
			<xsd:element name="numberOfRefills" type="xsd:int" minOccurs="0"/>
			<xsd:element name="prescriptionDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="prescriptionExpirationDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="renewalReasonCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SIGAdditionalInformation" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SIGCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SIGDescriptionPatientLanguage" type="xsd:string" minOccurs="0"/>
			<xsd:element name="prescriptionDispensableFlag" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Prescription Dispensable Indicator
This generally mirrors the status for the prescription, but in some circumstances may be changed to 'aborted' while the prescription is still active. When this occurs, it means the prescription may no longer be dispensed, though it may still be administered. Allows a prescriber to say "Finish what you have on hand, but don't get any more."
Because the status should always be known, this element is mandatory.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fillStatusEffectiveDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="legacyFlag" type="xsd:boolean" default="false" minOccurs="0"/>
			<xsd:element name="holdEndDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="transferableCode" type="xsd:boolean" default="false" minOccurs="0"/>
			<xsd:element name="administrationForm" type="xsd:string" minOccurs="0"/>
			<xsd:element name="administrationPeriodStartDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="statusChangeType" type="xsd:string" minOccurs="0"/>
			<xsd:element name="statusChangeReasonCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="updateEffectiveStartDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="prescriptionValidFromDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="administrationPeriodEndDate" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="daysInterval" type="xsd:int" minOccurs="0"/>
			<xsd:element name="updateEffectiveEndDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="createTimestamp" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="daysSupplyPrescribed" type="xsd:int" minOccurs="0"/>
			<xsd:element name="quantityPrescribed" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="noMoreDispensesAllowedFlag" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="isBatchFlag" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="serviceLocation" type="ehlth:Location" minOccurs="0"/>
			<xsd:element name="prescribedPack" type="ehlth:Pack" minOccurs="0"/>
			<xsd:element name="substitutionInitiator" type="ehlth:SubstitutionInitiator" minOccurs="0"/>
			<xsd:element name="fillStatusCode" type="ehlth:RxFillStatus" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>OPtions include LOG,FILL, DEPL, CAN,POST,ENT.

Used by Campaign.

Used to determine eligibility and status of prescriptions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="linkedrx" type="ehlth:Prescription" minOccurs="0"/>
			<xsd:element name="patient" type="ehlth:Patient" minOccurs="0"/>
			<xsd:element name="prescriber" type="ehlth:Prescriber" minOccurs="0"/>
			<xsd:element name="treatmentType" type="ehlth:TreatmentType" minOccurs="0"/>
			<xsd:element name="dispense" type="ehlth:Dispense" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Source" type="ehlth:Source" minOccurs="0"/>
			<xsd:element name="note" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="prescribedDrug" type="ehlth:Drug" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>prescribed drugs as on the prescription</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="prescribedCompound" type="ehlth:Compound" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Compounds on the prescription</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="discontinuedReasonCode" type="ehlth:DiscontinuedReason" minOccurs="0"/>
			<xsd:element name="resumeReasonCode" type="ehlth:PrescriptionResumeReason" minOccurs="0"/>
			<xsd:element name="recorder" type="ehlth:Recorder" minOccurs="0"/>
			<xsd:element name="holdReasonCode" type="ehlth:PrescriptionHoldReasonTypeDisplay" minOccurs="0"/>
			<xsd:element name="responsiblePerson" type="ehlth:ResponsiblePerson" minOccurs="0"/>
			<xsd:element name="supervisor" type="ehlth:Supervisor" minOccurs="0"/>
			<xsd:element name="firstSummaryDispense" type="ehlth:Dispense" minOccurs="0"/>
			<xsd:element name="lastSummaryDispense" type="ehlth:Dispense" minOccurs="0"/>
			<xsd:element name="firstDispenseInstruction" type="ehlth:Dispense" minOccurs="0"/>
			<xsd:element name="fillStatusSubCode" type="ehlth:RxFillStatus" minOccurs="0"/>
			<xsd:element name="forwardRx" type="ehlth:Prescription" minOccurs="0"/>
			<xsd:element name="rxParams" type="ehlth:RxParams" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="PrescriptionHoldReason">
