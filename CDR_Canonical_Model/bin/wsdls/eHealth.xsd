					<xsd:documentation>The request originated in the Loblaw Store</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IVR">
				<xsd:annotation>
					<xsd:documentation>The request originated from the IVR channel</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Prescriber">
				<xsd:annotation>
					<xsd:documentation>The request originated from the Prescriber</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="eHealth">
				<xsd:annotation>
					<xsd:documentation>The request originated from the eHealth channel</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Accuro"/>
			<xsd:enumeration value="CustomerServiceRepresentative">
				<xsd:annotation>
					<xsd:documentation>The request originated from the CSR channel</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PatientConsent">
				<xsd:annotation>
					<xsd:documentation>The request originated as a result of Patient Consent</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
