			<xsd:enumeration value="Guest">
				<xsd:annotation>
					<xsd:documentation>The user was not logged into an account</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Account">
				<xsd:annotation>
					<xsd:documentation>The user was logged into their account</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Unknown">
				<xsd:annotation>
					<xsd:documentation>It is not known if the user was logged into their account or this value is not applicable.</xsd:documentation>
