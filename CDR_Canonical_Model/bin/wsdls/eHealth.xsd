			<xsd:element name="exceptionId" type="sdm:IdentityUUID" minOccurs="0"/>
			<xsd:element name="details" type="xsd:string" minOccurs="0"/>
			<xsd:element name="interactionIssueDocument" type="ehlth:ManagedIssueDocument" minOccurs="0"/>
			<xsd:element name="managedIssueDocument" type="ehlth:ManagedIssueDocument" minOccurs="0"/>
			<xsd:element name="instantiationDocument" type="ehlth:ManagedIssueDocument" minOccurs="0"/>
			<xsd:element name="priority" type="ehlth:Priority" minOccurs="0"/>
			<xsd:element name="severity" type="ehlth:Severity" minOccurs="0"/>
			<xsd:element name="exceptionType" type="ehlth:ExceptionType" minOccurs="0"/>
		</xsd:sequence>
