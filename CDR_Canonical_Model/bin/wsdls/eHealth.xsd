	<xsd:element name="Patient" type="ehlth:Patient"/>
	<xsd:complexType name="Patient">
		<xsd:annotation>
			<xsd:documentation>The individual who has been prescribed a particular drug or drug.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="dateOfBirth" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date of Birth.

Used by Campaign.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="deceaseddate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Id in consumer for this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>id the producer assigned to this instance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="isactive" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>FKA activeStatus</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="noKnownAllergyFlag" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="createTimestamp" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="lastUpdateTimestamp" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="person" type="ehlth:Person" minOccurs="0"/>
			<xsd:element name="allergy" type="ehlth:Allergy" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="genderTypeCode" type="ehlth:PatientGender" minOccurs="0"/>
			<xsd:element name="groupmembership" type="ehlth:GroupMembership" minOccurs="0"/>
			<xsd:element name="patientrefillreminder" type="ehlth:OptInOptions" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Options: Not Specified, Accept, Decline.

Used by Campaign.

Used to determine/confirm patients refill reminder preference.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="patienttype" type="ehlth:PatientType" minOccurs="0"/>
			<xsd:element name="consent" type="ehlth:Consent" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="note" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="adverseDrugReaction" type="ehlth:AdverseDrugReaction" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="patientMetrics" type="ehlth:PatientMetrics" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="patientIdentification" type="ehlth:PatientIdentification" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="patientMedicalCondition" type="ehlth:PatientMedicalCondition" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="patientObservation" type="ehlth:PatientObservation" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="InsuranceCoverage" type="ehlth:InsuranceCoverage" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="mobileNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="associationType" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Association Type is for linking Customer to Patient, Data Domain ( C - Caregiver, S - Self Consolidated)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="preference" type="CSE:Preference" minOccurs="0"/>
			<xsd:element name="store" type="ehlth:Store" minOccurs="0"/>
			<xsd:element name="verificationChannel" type="ehlth:PharmacyChannel" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Patient credentials  can be verified in DRx or Maple systems </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="languageCorrespondence" type="ehlth:Language" minOccurs="0"/>
			<xsd:element name="sigLanguage" type="ehlth:Language" minOccurs="0"/>
			<xsd:element name="monographLanguage" type="ehlth:Language" minOccurs="0"/>
			<xsd:element name="primaryPrescriber" type="ehlth:Prescriber" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="PatientGender">
		<xsd:annotation>
			<xsd:documentation>Identifies the gender of a person.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Female"/>
			<xsd:enumeration value="Male"/>
			<xsd:enumeration value="Undifferentiated"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="PatientIdentification" type="ehlth:PatientIdentification"/>
	<xsd:complexType name="PatientIdentification">
		<xsd:annotation>
			<xsd:documentation>Identification of the patient</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdentificationNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Identification number (alpha) given t the patient</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssuingAuthorityName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Authority that issued the number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdentificationTypeCode" type="ehlth:IdentificationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of identification: driverlicense, healthcard # et al</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="PatientMedicalCondition" type="ehlth:PatientMedicalCondition"/>
	<xsd:complexType name="PatientMedicalCondition">
		<xsd:annotation>
			<xsd:documentation>Describes a medical condition for a patient. A patient may have multiple conditions</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="conditionDescription" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Condition Description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="conditionStartDate" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date the condition was first recorded</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="conditionEndDate" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date the condition was last recorded</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="chronicConditionFlag" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates that the condition is chronic ( constant; habitual)

from HW -&gt; Canonical Mapping
Acute, NA -&gt; false
Chronic -&gt; true</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="conditionCreateTimestamp" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>timestamp when this entry was recorded</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="conditionActiveFlag" type="xsd:boolean" default="false">
				<xsd:annotation>
					<xsd:documentation>Indicates if the medical condition is act or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="updateReason" type="xsd:string" minOccurs="0"/>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="updateTimeStamp" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="updateRecorderTimestamp" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="conditionNote" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="conditionTypeDisplay" type="ehlth:MedicalConditionTypeTypeDisplay" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>a Coded  medical condition</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="serviceLocation" type="ehlth:Location" minOccurs="0"/>
			<xsd:element name="reporter" type="ehlth:Reporter" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Person name who reported the allergy for the patient</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="recorder" type="ehlth:Recorder" minOccurs="0"/>
			<xsd:element name="supervisor" type="ehlth:Supervisor" minOccurs="0"/>
			<xsd:element name="updateReasonType" type="ehlth:StatusChangeReason" minOccurs="0"/>
			<xsd:element name="note" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="linkedCondition" type="ehlth:PatientMedicalCondition" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="reportedDate" type="xsd:date" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="PatientMedicalConditionSeverity">
		<xsd:annotation>
			<xsd:documentation>the severity of a medical condition

*Domain*  hw(1,2)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Active">
				<xsd:annotation>
					<xsd:documentation>Tracking of the condition is active</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Completed">
				<xsd:annotation>
					<xsd:documentation>monitoring of the condition has ceased</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="PatientMetrics" type="ehlth:PatientMetrics"/>
	<xsd:complexType name="PatientMetrics">
		<xsd:sequence>
			<xsd:element name="vitalsDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The timestamp of when the set of vital observations was entered into the system.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="height" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The hieght of the patient expressed in centimeter. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="weight" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The value of the patient weight expressed in kilogram.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bloodGlucose" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The measured blood glucose level of the patient expressed in mmol/l </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="heightMeasurementFlag" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>True if the height was measured, false if it was stated.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="weightMeasurementFlag" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>True if the height was measured, false if it was stated.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bloodGlucoseAutomaticMeasurementFlag" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>True if the blood glucose measurement was automatically measured, false if measured manually.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="heartRate" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The patient's heart rate, expressed in beats per minute.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="respiratoryRate" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The patient's breathing rate, expressed in breaths per minute.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bodyTemperature" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The patient's body temperature, expressed in degrees Celsius.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="systolicBloodPressure" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The patient's blood pressure, expressed in mm of mercury systolic.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="diastolicBloodPressure" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The patient's blood pressure, expressed in mm of mercury diastolic.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="note" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="heightDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="weightDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="patientWeightUOMCode" type="ehlth:MetricsUnitOfMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>UOM - metric of old style</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="patientHeightUOMCode" type="ehlth:MetricsUnitOfMeasure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>UOM for the height</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="PatientObservation" type="ehlth:PatientObservation"/>
	<xsd:complexType name="PatientObservation">
		<xsd:annotation>
			<xsd:documentation>Medical observations about a patient's well being. Observations may be tied to other conditions/ allergies/ adt et al</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="createTimestamp" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>timestamp of the last update (or creation)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="observationValue" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The actual measurement itself</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="observationTimestamp" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When the observation was made. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="producerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>identifier from producer for the reaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="observationNumerator" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>In a observation represented as a fraction, the observation's numerator. E.g. Blood pressure systolic</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="observationDenominator" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>In a observation represented as a fraction, the observation's numerator. E.g. Blood pressure diastolic </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="consumerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>identifier from producer for the reaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="note" type="ehlth:Note" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>This is the note that was recorded against the reaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="supervisor" type="ehlth:Supervisor" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>first name of person which act as supervisor at the time of entry</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="recorder" type="ehlth:Recorder" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>first name of person creating the condition entry</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="patientObservationTypeCode" type="ehlth:ClinicalObservationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>the type of measurement that was taken to measure the reaction 

Distinguishes between the kinds of measurable observation that could be the trigger for clinical issue detection. Measurable observation types include: Lab Result, Height, Weight, and other measurable information about a person that may be deemed as a possible trigger for clinical issue detection. 
Indicates the type of observation record being referenced. The attribute is mandatory because it is essential to interpreting the rest of the information on the class. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="location" type="ehlth:Location" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>location where the condition was recorded</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="observationMeasurementModifierCode" type="ehlth:ObservationMeasurementModifier" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="PatientType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="regular"/>
			<xsd:enumeration value="animal"/>
			<xsd:enumeration value="pharmacy"/>
			<xsd:enumeration value="prescriber/OfficeUse"/>
			<xsd:enumeration value="healthCareFacility"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="Person" type="ehlth:Person"/>
	<xsd:complexType name="Person">
