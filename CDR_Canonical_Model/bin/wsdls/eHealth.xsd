			<xsd:element name="address" type="ehlth:Address" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="dateOfBirth" type="xsd:date" minOccurs="0"/>
			<xsd:element name="deceaseddate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="lastUpdateTimestamp" type="xsd:dateTime" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
