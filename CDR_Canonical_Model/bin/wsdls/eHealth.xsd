	<xsd:complexType name="PharmacyChannel">
		<xsd:sequence>
			<xsd:element name="channelType" type="ehlth:ChannelType"/>
			<xsd:element name="communicationMode" type="ehlth:CommunicationModeType"/>
			<xsd:element name="engagment" type="ehlth:EngagementType"/>
